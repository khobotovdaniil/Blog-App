{
  "expo": {
    "name": "Blog",
    "slug": "2-blog-rn",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/rn-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8d2ab1dd-4332-4cb3-b9e9-48c1d0314948"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.khobotovdev.blog-rn-app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to save photos.",
        "NSPhotoLibraryAddUsageDescription": "This app uses library for saving photos",
        "NSPhotoLibraryUsageDescription": "This app uses library for saving photos"
      }
    },
    "android": {
      "package": "com.khobotovdev.blog_rn_app",
      "versioCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8d2ab1dd-4332-4cb3-b9e9-48c1d0314948"
      }
    },
    "owner": "dinich",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
